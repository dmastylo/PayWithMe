<% unless @event_user.nil? %>
    $('#event-<%= @event_user.event.id %>').html('<i class="icon-check icon-white"></i>');
    $('#upcoming-events').append('<%= j render(partial: "events/profile_event", object: @event_user.event, as: :event) %>');
    $('#member-event-count').html('<%= @event_user.member.member_events.count %>');
<% end %>